{
    "name": "morpheus-client",
    "script": "serve",
    "env": {
    "PM2_SERVE_PATH": "/opt/morpheus-client/build",
        "PM2_SERVE_PORT": 5000,
        "NODE_ENV": "production"
    },
    "instances": "1",
    "args": ["NODE_ENV=production"],
    "error_file": "/var/log/morpheus-client-error.log",
    "out_file": "/var/log/morpheus-client-output.log",
    "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
    "merge_logs": true
}