{
    "name": "morpheus-server",
    "script": "server.js",
    "instances": "1",
    "args": ["NODE_ENV=production"],
    "env_production": {
        "NODE_ENV": "production",
        "PORT": "3030"
    },
    "error_file": "/var/log/morpheus-server-error.log",
    "out_file": "/var/log/morpheus-server-output.log",
    "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
    "merge_logs": true,
    "cwd": "/opt/morpheus-server"
}